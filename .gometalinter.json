{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "Exclude": [
    ".*\\.pb\\.go",
    "fetch\\.go:.*::error: unrecognized printf verb 'r'"
  ],
  "EnableGC": true,

  "Enable": [
    "gotype",
    "nakedret",
    "safesql",
    "unconvert"
  ],

  "Disabled": [
    "deadcode",
    "errcheck",
    "goconst",
    "goimports",
    "gas",
    "ineffassign",
    "interfacer",
    "maligned",
    "megacheck",
    "misspell",
    "structcheck",
    "unparam",
    "varcheck",
    "lll <- 'long lines' - just too annoying",
    "gocyclo <- ",
    "dupl <- "
  ]
}
